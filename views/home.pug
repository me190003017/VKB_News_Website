          a.dropdown-item(href='/sports') Sports
          a.dropdown-item(href='/business') Business
          a.dropdown-item(href='/health') Health
          .dropdown-divider
          a.dropdown-item(href='/science') Science
          a.dropdown-item(href='/entertainment') Entertainment





#carouselExampleCaptions.carousel.slide.carousel-fade(data-ride='carousel')
